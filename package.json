    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-class-static-block": "^7.28.3",
    "@expo/metro-runtime": "^5.0.4",
    "@types/react": "~19.0.14",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.10",
